package day13;
class Addition
{
	int c;
	int add(int x,int y)
    {
    	c = x+y;
    	return c;
    }


}
public class Demo1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Addition ref = new Addition();
		int a = 10;
		int b = 20;
		int res = ref.add(a, b);
		System.out.println(res);
	}

}
output
30
