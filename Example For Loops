public class ExampleForLoops {
//sum of n natural numbers 
	public static void main(String[] args) {
		// TODO Auto-generated method stub
int n = 5;
int sum = 0;
for(int i=1; i<n; i++)
{
	sum = sum + i;
	System.out.println(sum);
}
System.out.println("=================================");
int j =  1;
while (j<=5)
{
	System.out.println(j);
      j++;
}
	}

}
output
1
3
6
10
=================================
1
2
3
4
5
